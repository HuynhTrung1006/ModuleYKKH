@model IEnumerable<YKKH_Module.Models.TRUNGTHUONG>

@{
    ViewBag.Title = "IndexTrungThuong";
    Layout = "~/Views/Shared/_LayoutQT.cshtml";
    List<YKKH_Module.Models.TRUNGTHUONG> ds = ViewBag.DSTT;
}

