<script src="~/Scripts/DataTables/jquery.dataTables.js"></script>
<link href="~/Scripts/DataTables/bootstrap.min.css" rel="stylesheet" />
<link href="~/Scripts/DataTables/dataTables.bootstrap5.min.css" rel="stylesheet" />
<link href="~/Scripts/DataTables/jquery.dataTables.css" rel="stylesheet" />
@if (Session["User"] == null)
{
    <script>
        var result = confirm("Không thể truy cập do chưa đăng nhập hệ thống");
    </script>
}
else
{
    <div class="container-fluid">
        <div class="card-header">
            <h3 class="card-title text-black-50"><b>Quản lý Trúng Thưởng</b></h3>
        </div>
        <div class="card-body">
            <table class="table table-striped" id="TTTable">
                <thead>
                    <tr class="text-center">
                        <th>
                            ID
                        </th>
                        <th>
                            Tên Chủ đề
                        </th>
                        <th>
                            Mã Khách Hàng
                        </th>
                        
                        <th>
                            Số dự đoán
                        </th>
                        <th>
                            Kết quả
                        </th>
                        <th></th>
                    </tr>
                </thead>
                <tbody>
                    @foreach (var item in ds)
                    {
                    <tr class="text-center">
                        <td>
                            @Html.DisplayFor(modelItem => item.maTT)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.maCD)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.maKH)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.songuoithamgia)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.socaudung)
                        </td>
                    </tr>
                    }
                </tbody>

            </table>
        </div>
       
    </div>
   
        


}

